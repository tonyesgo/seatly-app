{
  "expo": {
    "name": "Seatly",
    "slug": "seatly",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "seatly",
    "userInterfaceStyle": "automatic",

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#030C27"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tonyesgo.seatly",
      "buildNumber": "1.0.0"
    },

    "android": {
      "package": "com.tonyesgo.seatly",
      "versionCode": 6,
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            { "scheme": "seatly", "host": "payment", "pathPrefix": "/success" },
            { "scheme": "seatly", "host": "payment", "pathPrefix": "/failure" },
            { "scheme": "seatly", "host": "payment", "pathPrefix": "/pending" }
          ]
        }
      ]
    },

    "androidStatusBar": { "backgroundColor": "#030C27" },

    "web": { "favicon": "./assets/images/favicon.png" },

    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.20"
          }
        }
      ]
    ],

    "experiments": { "typedRoutes": false },

    "extra": {
      "eas": { "projectId": "e3dbe5b7-ad0c-42b1-a1de-5968dc5c840a" }
    },

    "owner": "tonyesgo",
    "sdkVersion": "53.0.0",
    "platforms": ["ios", "android", "web"]
  }
}

